{
  "version": 2,
  "project": {
    "id": %%TEST_KBC_PROJECT_ID%%,
    "apiHost": "%%TEST_KBC_STORAGE_API_HOST%%"
  },
  "sortBy": "path",
  "naming": {
    "branch": "{branch_name}",
    "config": "{component_type}/{component_id}/{config_name}",
    "configRow": "rows/{config_row_name}",
    "schedulerConfig": "schedules/{config_name}",
    "sharedCodeConfig": "_shared/{target_component_id}",
    "sharedCodeConfigRow": "codes/{config_row_name}",
    "variablesConfig": "variables",
    "variablesValuesRow": "values/{config_row_name}"
  },
  "allowedBranches": [
    "__all__"
  ],
  "ignoredComponents": [],
  "templates": {
    "repositories": [
      {
        "type": "dir",
        "name": "keboola",
        "path": "../repository",
        "url": "https://github.com/keboola/keboola-as-code-templates.git",
        "ref": "main"
      }
    ]
  },
  "branches": [
    {
      "id": 12345,
      "path": "main"
    }
  ],
  "configurations": [
    {
      "branchId": 12345,
      "componentId": "keboola.shared-code",
      "id": "%s",
      "path": "_shared/keboola.python-transformation-v2",
      "rows": [
        {
          "id": "%s",
          "path": "codes/code-with-variables"
        },
        {
          "id": "%s",
          "path": "codes/my-code-1"
        },
        {
          "id": "%s",
          "path": "codes/my-code-2"
        }
      ]
    },
    {
      "branchId": 12345,
      "componentId": "keboola.variables",
      "id": "%s",
      "path": "variables",
      "relations": [
        {
          "configId": "%s",
          "rowId": "%s",
          "type": "sharedCodeVariablesFor"
        }
      ],
      "rows": []
    },
    {
      "branchId": 12345,
      "componentId": "ex-generic-v2",
      "id": "%s",
      "path": "extractor/ex-generic-v2/empty",
      "rows": []
    },
    {
      "branchId": 12345,
      "componentId": "ex-generic-v2",
      "id": "%s",
      "path": "extractor/ex-generic-v2/om-config",
      "rows": []
