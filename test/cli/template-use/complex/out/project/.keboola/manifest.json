    {
      "branchId": 12345,
      "componentId": "keboola.ex-db-mysql",
      "id": "%s",
      "path": "extractor/keboola.ex-db-mysql/with-rows",
      "rows": [
        {
          "id": "%s",
          "path": "rows/disabled"
        },
        {
          "id": "%s",
          "path": "rows/test-view"
        },
        {
          "id": "%s",
          "path": "rows/users"
        }
      ]
    },
    {
      "branchId": 12345,
