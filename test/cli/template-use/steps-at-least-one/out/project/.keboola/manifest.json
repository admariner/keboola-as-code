{
  "version": 2,
  "project": {
    "id": %%TEST_KBC_PROJECT_ID%%,
    "apiHost": "%%TEST_KBC_STORAGE_API_HOST%%"
  },
  "sortBy": "path",
  "naming": {
    "branch": "{branch_name}",
    "config": "{component_type}/{component_id}/{config_name}",
    "configRow": "rows/{config_row_name}",
    "schedulerConfig": "schedules/{config_name}",
    "sharedCodeConfig": "_shared/{target_component_id}",
    "sharedCodeConfigRow": "codes/{config_row_name}",
    "variablesConfig": "variables",
    "variablesValuesRow": "values/{config_row_name}"
  },
  "allowedBranches": [
    "__all__"
  ],
  "ignoredComponents": [],
  "templates": {
    "repositories": [
      {
        "type": "dir",
        "name": "keboola",
        "path": "../repository",
        "url": "https://github.com/keboola/keboola-as-code-templates.git",
        "ref": "main"
      }
    ]
  },
  "branches": [
    {
      "id": 12345,
      "path": "main",
      "metadata": {
        "KBC.KAC.templates.instances": "[{\"instanceId\":\"%s\",\"instanceName\":\"My Instance\",\"templateId\":\"my-template-id\",\"repositoryName\":\"keboola\",\"version\":\"1.0.0\",\"created\":{\"date\":\"%s\",\"tokenId\":\"%s\"},\"updated\":{\"date\":\"%s\",\"tokenId\":\"%s\"}}]"
      }
    }
  ],
  "configurations": [
    {
      "branchId": 12345,
      "componentId": "ex-generic-v2",
      "id": "%s",
      "path": "extractor/ex-generic-v2/shoptet",
      "metadata": {
        "KBC.KAC.templates.configId": "{\"idInTemplate\":\"shoptet\"}",
        "KBC.KAC.templates.configInputs": "[{\"input\":\"shoptet-password\",\"key\":\"parameters.password\"},{\"input\":\"shoptet-username\",\"key\":\"parameters.username\"}]",
        "KBC.KAC.templates.instanceId": "%s",
        "KBC.KAC.templates.repository": "keboola",
        "KBC.KAC.templates.templateId": "my-template-id"
      },
      "rows": []
    }
  ]
}
