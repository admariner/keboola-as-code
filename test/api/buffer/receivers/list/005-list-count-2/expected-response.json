{
  "receivers": [
    {
      "id": "receiver-a",
      "url": "https://buffer.keboola.local/v1/import/%d/receiver-a/%s",
      "name": "Receiver A",
      "exports": []
    },
    {
      "id": "receiver-b-my-id",
      "url": "https://buffer.keboola.local/v1/import/%d/receiver-b-my-id/#/%s",
      "name": "Receiver B",
      "exports": [
        {
          "id": "export-1-my-id",
          "receiverId": "receiver-b-my-id",
          "name": "Export 1",
          "mapping": {
            "tableId": "in.c-bucket.table1",
            "incremental": true,
            "columns": [
              {
                "type": "id"
              },
              {
                "type": "template",
                "template": {
                  "language": "jsonnet",
                  "undefinedValueStrategy": "error",
                  "content": "body.key+\":\"+body.value",
                  "dataType": "STRING"
                }
              }
            ]
          },
          "conditions": {
            "count": 1000,
            "size": "1MB",
            "time": "5m0s"
          }
        },
        {
          "id": "export-2",
          "receiverId": "receiver-b-my-id",
          "name": "Export 2",
          "mapping": {
            "tableId": "in.c-bucket.table2",
            "incremental": false,
            "columns": [
              {
                "type": "id"
              },
              {
                "type": "body"
              },
              {
                "type": "headers"
              }
            ]
          },
          "conditions": {
            "count": 123,
            "size": "456B",
            "time": "30s"
          }
        }
      ]
    }
  ]
}

