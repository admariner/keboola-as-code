{
  "id": "my-receiver",
  "url": "https://buffer.keboola.local/v1/import/%%TEST_KBC_PROJECT_ID%%/my-receiver/%s",
  "name": "My Receiver",
  "exports": [
    {
      "id": "export-1",
      "receiverId": "my-receiver",
      "name": "Export 1",
      "mapping": {
        "tableId": "in.c-bucket.table1",
        "incremental": true,
        "columns": [
          {
            "primaryKey": false,
            "type": "id",
            "name": "id"
          },
          {
            "primaryKey": false,
            "type": "template",
            "name": "template",
            "template": {
              "language": "jsonnet",
              "content": "body.key+\":\"+body.value"
            }
          }
        ]
      },
      "conditions": {
        "count": 10000,
        "size": "5MB",
        "time": "5m0s"
      }
    },
    {
      "id": "export-2",
      "receiverId": "my-receiver",
      "name": "Export 2",
      "mapping": {
        "tableId": "in.c-bucket.table2",
        "incremental": false,
        "columns": [
          {
            "primaryKey": false,
            "type": "id",
            "name": "id"
          },
          {
            "primaryKey": false,
            "type": "body",
            "name": "body"
          },
          {
            "primaryKey": false,
            "type": "headers",
            "name": "headers"
          }
        ]
      },
      "conditions": {
        "count": 123,
        "size": "456B",
        "time": "30s"
      }
    }
  ]
}

