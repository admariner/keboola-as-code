{
  "id": "receiver-1",
  "url": "https://buffer.keboola.local/v1/import/%%TEST_KBC_PROJECT_ID%%/receiver-1/%s",
  "name": "Receiver 1",
  "exports": [
    {
      "id": "export-1",
      "receiverId": "receiver-1",
      "name": "Export 1",
      "mapping": {
        "tableId": "in.c-bucket.table",
        "incremental": true,
        "columns": [
          {
            "type": "id",
            "name": "id"
          },
          {
            "type": "body",
            "name": "body"
          },
          {
            "type": "headers",
            "name": "headers"
          },
          {
            "type": "ip",
            "name": "ip"
          },
          {
            "type": "datetime",
            "name": "datetime"
          },
          {
            "type": "template",
            "name": "template",
            "template": {
              "language": "jsonnet",
              "undefinedValueStrategy": "error",
              "content": "BodyPath('col1')+\":\"+BodyPath('col2')"
            }
          }
        ]
      },
      "conditions": {
        "count": 1000,
        "size": "1MB",
        "time": "5m0s"
      }
    }
  ]
}
