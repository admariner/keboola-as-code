{
  "path": "/v1/receivers",
  "method": "POST",
  "headers": {
    "Content-Type": "application/json",
    "X-StorageApi-Token": "%%TEST_KBC_STORAGE_API_TOKEN%%"
  },
  "body": {
    "id": "receiver-1",
    "name": "Receiver 1",
    "exports": [
      {
        "exportId": "export-1",
        "name": "Export 1",
        "mapping": {
          "tableId": "in.c-bucket.table",
          "incremental": true,
          "columns": [
            {
              "type": "id",
              "name": "id"
            },
            {
              "type": "body",
              "name": "body"
            },
            {
              "type": "headers",
              "name": "headers"
            },
            {
              "type": "ip",
              "name": "ip"
            },
            {
              "type": "datetime",
              "name": "datetime"
            },
            {
              "type": "template",
              "name": "template",
              "template": {
                "language": "jsonnet",
                "content": "Body('col1')+\":\"+Body('col2')"
              }
            }
          ]
        }
      }
    ]
  }
}
