{
  "path": "/v1/receivers/my-receiver/exports",
  "method": "POST",
  "headers": {
    "Content-Type": "application/json"
  },
  "body": {
    "name": "My Export 2",
    "mapping": {
      "tableId": "in.c-bucket.table",
      "columns": [
        {
          "type": "id",
          "name": "id"
        },
        {
          "type": "template",
          "name": "template",
          "template": {
            "language": "jsonnet",
            "undefinedValueStrategy": "error",
            "content": "body.key+\":\"+body.value"
          }
        }
      ]
    }
  }
}

