{
  "path": "/v1/receivers",
  "method": "POST",
  "headers": {
    "Content-Type": "application/json",
    "X-StorageApi-Token": "%%TEST_KBC_STORAGE_API_TOKEN%%"
  },
  "body": {
    "name": "My Receiver",
    "exports": [
      {
        "name": "Export 1",
        "mapping": {
          "tableId": "in.c-bucket.table1",
          "incremental": true,
          "columns": [
            {
              "type": "id"
            },
            {
              "type": "template",
              "template": {
                "language": "jsonnet",
                "undefinedValueStrategy": "error",
                "content": "body.key+\":\"+body.value"
              }
            }
          ]
        }
      },
      {
        "name": "Export 2",
        "mapping": {
          "tableId": "in.c-bucket.table2",
          "incremental": false,
          "columns": [
            {
              "type": "id"
            },
            {
              "type": "body"
            },
            {
              "type": "headers"
            }
          ]
        },
        "conditions": {
          "count": 123,
          "size": "456B",
          "time": "30s"
        }
      }
    ]
  }
}
