{
  "path": "/v1/receivers/my-receiver/exports/my-export-1",
  "method": "PATCH",
  "headers": {
    "Content-Type": "application/json",
    "X-StorageApi-Token": "%%TEST_KBC_STORAGE_API_TOKEN%%"
  },
  "body": {
    "mapping": {
      "tableId": "in.c-bucket.table",
      "incremental": false,
      "columns": [
        {
          "type": "id",
          "name": "id"
        },
        {
          "type": "template",
          "name": "template",
          "template": {
            "language": "jsonnet",
            "content": "body.key+\":\"+body.value"
          }
        }
      ]
    }
  }
}

