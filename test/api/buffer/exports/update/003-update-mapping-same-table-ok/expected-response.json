{
  "id": "my-export-1",
  "receiverId": "my-receiver",
  "name": "My Export 1 Updated",
  "mapping": {
    "tableId": "in.c-bucket.table",
    "incremental": false,
    "columns": [
      {
        "type": "id",
        "name": "id"
      },
      {
        "type": "template",
        "name": "template",
        "template": {
          "language": "jsonnet",
          "undefinedValueStrategy": "error",
          "content": "body.key+\":\"+body.value"
        }
      }
    ]
  },
  "conditions": {
    "count": 500,
    "size": "100KB",
    "time": "1m0s"
  }
}

