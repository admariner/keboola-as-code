{
  "path": "/v1/receivers/my-receiver/exports",
  "method": "POST",
  "headers": {
    "Content-Type": "application/json",
    "X-StorageApi-Token": "%%TEST_KBC_STORAGE_API_TOKEN%%"
  },
  "body": {
    "exportId": "my-export-2",
    "name": "My Export 2",
    "mapping": {
      "tableId": "in.c-bucket.table",
      "incremental": true,
      "columns": [
        {
          "type": "id"
        },
        {
          "type": "template",
          "template": {
            "language": "jsonnet",
            "undefinedValueStrategy": "error",
            "content": "body.key+\":\"+body.value"
          }
        }
      ]
    },
    "conditions": {
      "count": 500,
      "size": "100kB",
      "time": "1m"
    }
  }
}

